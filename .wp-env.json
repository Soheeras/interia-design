{
	"$schema": "https://schemas.wp.org/trunk/wp-env.json",
	"core": "WordPress/WordPress",
	"port": 8988,
	"testsPort": 8989,
	"plugins": [ "." ],
	"config": {
		"WP_UPLOAD_MAX_FILESIZE": "128M",
		"WP_MEMORY_LIMIT": "256M",
		"WP_ENVIRONMENT_TYPE": "development",
		"WP_DEVELOPMENT_MODE": "all"
	},
	"env": {
		"development": {
			"phpmyadminPort": 9000
		},
		"tests": {
			"phpmyadminPort": 9001
		}
	}
}
